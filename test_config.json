    "validation_split": 0.2,
    "early_stopping_patience": 5,
    "gradient_clipping": 1.0,
    "mixed_precision": false,
    "device": "cpu"
  },
  "data": {
    "num_samples": 100,
    "noise_level": 0.01
  },
  "physics": {
    "physics_weight": 0.5,
    "boundary_weight": 1.0,
    "initial_weight": 1.0
  }
}