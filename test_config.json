    "hidden_layers": [64, 32, 16],
    "dropout_rate": 0.1,
    "batch_norm": true,
    "activation": "ReLU"
  },
  "training": {
    "epochs": 3,
    "batch_size": 16,
    "learning_rate": 0.001,
